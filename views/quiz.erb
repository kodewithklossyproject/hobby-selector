    <form id="my-form" method="post" action="/results">
      <section id="womp">
       <div id="question_1">

        <h3> Are you an indoor or outdoor person?</h3>
        <label for="indoor"> Indoors </label>
        <input id="indoor" type="radio" name="outside" value=100>
        <label> Outdoors </label>
        <input type="radio" name="outside" value=207>
      </div>
    </section>

    <section id="womp2">
      <div id="question_2"><!--question 2-->
