        <input type="radio" name="subject" value=25>
        <label> Science </label>
        <input type="radio" name="subject" value=35>
        <label> P.E. </label>
        <input type="radio" name="subject" value=45>
      </div>
    </section>


    <section id="womp3">
      <div id="picture_3"> <!--question 3--->
